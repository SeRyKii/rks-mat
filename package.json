		"@editorjs/quote": "^2.5.0",
		"@editorjs/raw": "^2.4.0",
		"@editorjs/table": "^2.2.1",
		"@iconify/svelte": "^3.1.4",
		"@skeletonlabs/skeleton": "^1.7.1",
